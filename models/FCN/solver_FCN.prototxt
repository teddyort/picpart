net: "FCN/train_FCN.prototxt"
test_initialization: true
test_interval: 100
display: 10
average_loss: 20
lr_policy: "fixed"
# lr for unnormalized softmax
base_lr: 1e-14
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 25000
weight_decay: 0.0005
snapshot: 2000
snapshot_prefix: "../snapshots/snapshot"
