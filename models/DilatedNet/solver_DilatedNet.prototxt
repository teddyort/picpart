net: "DilatedNet/train_DilatedNet.prototxt"
test_initialization: true
test_iter: 1
test_interval: 10
test_compute_loss: true
display: 10
max_iter: 15000
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 5000
momentum: 0.9
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "../snapshots/snapshot"
